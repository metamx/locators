{
  "name": "locators",
  "version": "1.0.1",
  "description": "A collection of promise based service discovery 'locators'",
  "keywords": [
    "zookeeper",
    "service discovery"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "http://github.com/metamx/locators.git"
  },
  "main": "build/index.js",
  "typescript": {
    "definition": "build/locators.d.ts"
  },
  "scripts": {
    "dts": "dts-generator --name locators --out build/locators.d.ts --project `(pwd)` --main locatorsld/index",
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "typings": "typings install",
    "build": "npm run typings && npm run lint && npm run compile && npm run dts ",
    "prepublish": "npm run build",
    "test": "node_modules/.bin/mocha"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "node-zookeeper-client": "^0.2.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "dts-generator": "^2.0.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  }
}
